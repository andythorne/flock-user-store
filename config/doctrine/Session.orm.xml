<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="App\Entity\Session" table="session">
        <id name="sessId" type="string">

        </id>
        <field name="sessData" type="blob" nullable="false"/>
        <field name="sessTime" type="integer" nullable="false">
            <options>
                <option name="unsigned">true</option>
            </options>
        </field>
        <field name="sessLifetime" type="integer" nullable="false"/>
    </entity>
</doctrine-mapping>
